generator client {
  provider        = "prisma-client-js"
  output         = "../src/generated/prisma"
  previewFeatures = ["relationJoins"]  // Updated from "relationMode"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"  // Critical for Supabase compatibility
}

model User {
  id    String @id
  email String @unique
}

